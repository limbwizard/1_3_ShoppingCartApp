# Basic Application Settings
REACT_APP_API_URL=https://myapi.example.com
REACT_APP_GOOGLE_ANALYTICS_ID=UA-12345678-9

# Authentication
REACT_APP_AUTH_ENDPOINT=https://myapi.example.com/auth
REACT_APP_AUTH_SECRET=your_auth_secret_here

# Database Connection (if applicable for direct connections or server-side use)
DB_HOST=localhost
DB_USER=myUser
DB_PASS=myPassword
DB_NAME=shoppingCartApp

# External APIs
REACT_APP_PAYMENT_SERVICE_URL=https://payments.example.com
REACT_APP_SHIPPING_SERVICE_URL=https://shipping.example.com

# Feature Flags
REACT_APP_FEATURE_NEW_CHECKOUT=enabled
REACT_APP_FEATURE_PRODUCT_REVIEWS=disabled
